<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bitcamp.devlog.mapper.Visit">

    <select id="findByAccountIdAndVisitDate" resultType="long" parameterType="Map">
        select count from visit where account_id=#{accountId} and visit_date=#{visitDate}
    </select>
    <select id="findByAccountId" resultType="Visit" parameterType="String">
        select * from visit where account_id=#{accountId}
    </select>

</mapper>